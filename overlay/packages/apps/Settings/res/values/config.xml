<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Defines the location of the fingerprint sensor on the device
         0 = back
         1 = front
         2 = left side
         3 = right side
    -->
    <integer name="config_fingerprintSensorLocation">3</integer>

    <!-- Battery Health Nodes -->
    <bool name="config_supportBatteryHealth">true</bool>
    <string name="config_batCurCap" translatable="false">/sys/class/power_supply/bms/charge_counter</string>
    <string name="config_batDesCap" translatable="false">/sys/class/power_supply/bms/charge_full</string>
    <string name="config_batChargeCycle" translatable="false">/sys/class/power_supply/bms/cycle_count</string>

    <!-- Whether to show min/max refresh rate in display settings -->
    <bool name="config_show_refresh_rate_controls">true</bool>

    <!-- The CPU temperature divider, if needed -->
    <integer name="config_cpuTempDivider" translatable="false">1000</integer>

    <!-- FPSInfoService FPS node file path -->
    <string name="config_fpsInfoSysNode" translatable="false">/sys/class/drm/sde-crtc-0/measured_fps</string>

    <!-- Show Force LTE_CA toggle -->
    <bool name="config_showForceLteCaToggle" translatable="false">true</bool>
</resources>
